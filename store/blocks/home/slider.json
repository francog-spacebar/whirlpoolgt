{
  "flex-layout.row#slider-container": {
    "children": ["slider-layout#demo-images"]
  },
  "slider-layout#demo-images": {
    "children": [
      "image#slider-image1",
      "image#slider-image2",
      "image#slider-image3",
      "image#slider-image4",
      "image#slider-image5",
      "image#slider-image6",
      "image#slider-image7"
    ],
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "slideTransition": { "speed": 200, "delay": 0, "timing": "" },
      "autoplay": { "timeout": 5000, "stopOnHover": true },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "mobileOnly",
      "blockClass": "home-slider",
      "fullWidth": true
    }
  },
  "image#slider-image1": {
