{
  "flex-layout.row#category-refrigeradores": {
    "children": [
      "flex-layout.col#home-category-left-refrigeradores",
      "flex-layout.col#home-category-right-refrigeradores"
    ],
    "props": { "blockClass": "home-category-container" }
  },
  "rich-text#home-cat1-subtitle": {
    "props": {
      "text": "Xpert Energy Saver",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "font": "t-heading-4",
      "blockClass": "home-category-subtitle"
    }
  },
  "rich-text#home-cat1-content-text": {
    "props": {
      "text": "Mucho más que un buen compresor. 3 tecnologías, ahorro superior",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "font": "t-f6",
      "blockClass": "home-category-subtitle"
    }
  },
  "image#home-cat1-content-image": {
    "props": {
      "src": "https://www.whirlpool.mx/arquivos/home-refrigeracion.jpg",
      "blockClass": "content-image-small"
    }
  },
  "rich-text#home-cat1-content-link": {
    "props": {
      "text": "[Conoce más](#)",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-f6",
      "blockClass": "home-category-link"
    }
  },
  "rich-text#category-refrigeradores-title": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "Refrigeradores",
      "font": "t-heading-2",
      "blockClass": "home-category-title"
    }
  },

  "flex-layout.row#home-category-big-image1": {
    "children": ["image#category-refrigeradores1"]
  },
  "flex-layout.row#home-category-small-image-container1": {
    "children": [
      "flex-layout.col#home-category1-small-image1",
      "flex-layout.col#home-category1-small-image2"
    ]
  },
  "flex-layout.col#home-category1-small-image1": {
    "children": ["image#category-refrigeradores2"],
    "props": {
      "width": "60%"
    }
  },
  "flex-layout.col#home-category1-small-image2": {
    "children": ["image#category-refrigeradores3"],
    "props": {
      "width": "40%",
      "marginLeft": "4"
    }
  },

  "flex-layout.col#home-category-left-refrigeradores": {
    "children": [
      "rich-text#category-refrigeradores-title",
      "rich-text#home-cat1-subtitle",
      "rich-text#home-cat1-content-text",
      "image#home-cat1-content-image",
      "rich-text#home-cat1-content-link"
    ],
    "props": {
      "blockClass": "home-category-info",
      "horizontalAlign": "center"
    }
  },
  "flex-layout.col#home-category-right-refrigeradores": {
    "children": [
      "flex-layout.row#home-category-big-image1",
      "flex-layout.row#home-category-small-image-container1"
    ],
    "props": {
      "blockClass": "home-category-right_layout"
    }
  },
  "image#category-refrigeradores1": {
    "props": {
      "src": "https://tiendaswhirlpoold2c.vteximg.com.br/arquivos/_home-refrigeracion_01.jpg",
      "blockClass": "big"
    }
  },
  "image#category-refrigeradores2": {
    "props": {
      "src": "https://tiendaswhirlpoold2c.vteximg.com.br/arquivos/_home-refrigeracion_02.jpg",
      "blockClass": "small-lg"
    }
  },
  "image#category-refrigeradores3": {
    "props": {
      "src": "https://tiendaswhirlpoold2c.vteximg.com.br/arquivos/_home-refrigeracion_03.jpg",
      "blockClass": "small-sm"
    }
  }
}
