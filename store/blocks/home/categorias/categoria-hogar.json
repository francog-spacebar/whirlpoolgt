{
  "flex-layout.row#category-hogar": {
    "children": [
      "flex-layout.col#home-category-left-hogar",
      "flex-layout.col#home-category-right-hogar"
    ],
    "props": { "blockClass": "home-category-container" }
  },
  "rich-text#home-cat4-subtitle": {
    "props": {
      "text": "Xpert Energy Saver Inverter.",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "font": "t-heading-4",
      "blockClass": "home-category-subtitle"
    }
  },
  "rich-text#home-cat4-content-text": {
    "props": {
      "text": "70% de ahorro de energía.",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "font": "t-f6",
      "blockClass": "home-category-subtitle"
    }
  },
  "image#home-cat4-content-image": {
    "props": {
      "src": "https://www.whirlpool.mx/arquivos/_home-agua-aire.jpg",
      "blockClass": "content-image-small"
    }
  },
  "rich-text#home-cat4-content-link": {
    "props": {
      "text": "[Conoce más](#)",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-f6",
      "blockClass": "home-category-link"
    }
  },
  "rich-text#category-hogar-title": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "Hogar",
      "font": "t-heading-2",
      "blockClass": "home-category-title"
    }
  },

  "flex-layout.row#home-category-big-image4": {
    "children": ["image#category-hogar1"]
  },
  "flex-layout.row#home-category-small-image-container4": {
    "children": [
      "flex-layout.col#home-category4-small-image1",
      "flex-layout.col#home-category4-small-image2"
    ]
  },
  "flex-layout.col#home-category4-small-image1": {
    "children": ["image#category-hogar2"],
    "props":{
      "width":"60%",
      "marginRight":"4"
    }
  },
  "flex-layout.col#home-category4-small-image2": {
    "children": ["image#category-hogar3"],
    "props":{
      "width":"40%"
    }
  },
  "flex-layout.col#home-category-left-hogar": {
    "children": [
      "flex-layout.row#home-category-big-image4",
      "flex-layout.row#home-category-small-image-container4"
    ],
    "props": {
      "blockClass": "home-category-right_layout"
    }
  },
  "flex-layout.col#home-category-right-hogar": {
    "children": [
      "rich-text#category-hogar-title",
      "rich-text#home-cat4-subtitle",
      "rich-text#home-cat4-content-text",
      "image#home-cat4-content-image",
      "rich-text#home-cat4-content-link"
    ],
    "props": {
      "blockClass": "home-category-info",
      "horizontalAlign": "center"
    }
  },
  "image#category-hogar1": {
    "props": {
      "src": "https://tiendaswhirlpoold2c.vteximg.com.br/arquivos/_home-agua-aire_01.jpg",
      "blockClass": "big"
    }
  },
  "image#category-hogar2": {
    "props": {
      "src": "https://tiendaswhirlpoold2c.vteximg.com.br/arquivos/_home-agua-aire_02.jpg",
      "blockClass": "small-lg"
    }
  },
  "image#category-hogar3": {
    "props": {
      "src": "https://tiendaswhirlpoold2c.vteximg.com.br/arquivos/_home-agua-aire_03.jpg",
      "blockClass": "small-sm"
    }
  }
